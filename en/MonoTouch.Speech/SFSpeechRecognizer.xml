      <MemberSignature Language="C#" Value="public static MonoTouch.Speech.SFSpeechRecognizerAuthorizationStatus AuthorizationStatus { get; }" />
      <MemberSignature Language="ILAsm" Value=".property valuetype MonoTouch.Speech.SFSpeechRecognizerAuthorizationStatus AuthorizationStatus" />
